# build docker image
su -
cd /home/hsiehpinghan/git/boot/spring-cloud-starter-eureka-boot
./mvnw install dockerfile:build

# run in docker
docker run -e --net="host" -p 8080:8080 -t hsiehpinghan/spring-cloud-starter-eureka-boot:1.0.0

# test url (start spring-cloud-starter-eureka-server-book first)
run DiscoveryClientTest


