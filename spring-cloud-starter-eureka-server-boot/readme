# build docker image
su -
cd /home/hsiehpinghan/git/boot/spring-cloud-starter-eureka-server-boot
./mvnw install dockerfile:build

# run in docker
docker run -e "SPRING_PROFILES_ACTIVE=server_0" --net="host" -p 8761:8761 -t hsiehpinghan/spring-cloud-starter-eureka-server-boot:1.0.0
docker run -e "SPRING_PROFILES_ACTIVE=server_1" --net="host" -p 8762:8762 -t hsiehpinghan/spring-cloud-starter-eureka-server-boot:1.0.0
docker run -e "SPRING_PROFILES_ACTIVE=server_2" --net="host" -p 8763:8763 -t hsiehpinghan/spring-cloud-starter-eureka-server-boot:1.0.0

# test url
http://localhost:8761/
http://localhost:8762/
http://localhost:8763/