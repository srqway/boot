# build docker image
su -
cd /home/hsiehpinghan/git/boot/spring-cloud-starter-eureka-boot
./mvnw install dockerfile:build
docker login -u <<USERNAME>> -p <<PASSWORD>>
./mvnw install dockerfile:push

# run in docker
docker run -e "SPRING_PROFILES_ACTIVE=prod" --net="host" -p 8761:8761 -t hsiehpinghan/spring-cloud-starter-eureka-server-boot:1.0.0
docker run -e "SPRING_PROFILES_ACTIVE=prod" --net="host" -p 8080:8080 -t hsiehpinghan/spring-cloud-starter-eureka-boot:1.0.0

# test url
run DiscoveryClientTest


